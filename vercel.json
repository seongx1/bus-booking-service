{
  "version": 2,
  "builds": [
    {
      "src": "api/slack-proxy.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/slack-proxy",
      "dest": "/api/slack-proxy.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "functions": {
    "api/slack-proxy.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
